<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>

		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Authors>Alex Stroot</Authors>
		<Description>A library of common classes and extensions for validation in either front end or back end contexts. Extends 'FluentValidation'.</Description>
		<Copyright>2023</Copyright>
		<PackageProjectUrl>https://github.com/AlexLexicon/Lexicon.Common/tree/master/Lexicon.Common.Validation</PackageProjectUrl>
		<RepositoryUrl>https://github.com/AlexLexicon/Lexicon.Common</RepositoryUrl>
		<RepositoryType>github</RepositoryType>
		<PackageTags>fluentvalidation</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\Lexicon.Common.Validation.Abstractions\Lexicon.Common.Validation.Abstractions.csproj" />
	</ItemGroup>

</Project>
